
List = enum { Empty(), Node(value: Int, next: List) }

length(list: List): Int = match list {
  List.Node(value, next) => 1 + length(next)
  List.Empty() => 0
}

myList =
  List.Node(10, List.Node(8, List.Empty()))

main() = {
  printf("List: %j", myList);
  printf("Length: %j", length(myList));
  0
}
