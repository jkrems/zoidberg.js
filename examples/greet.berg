
TWO = 2

isOne(x) = match x {
  1: true
  else: false
}

mul(a, b) = { a * b }

square(x) = { mul(x, TWO) }

main(x: Int32): ExitCode = {
  match isOne(x) {
    true: printf("Was called with `1`")
    false: printf("Wasn't called with `1` but with `%d`", x)
  };
  printf("Result: %d", -square(x) + TWO * 10);
  0
}
